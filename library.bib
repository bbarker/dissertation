month = oct,
number = {6960},
pages = {798--804},
pmid = {14574403},
title = {{Genome-scale approaches to resolving incongruence in molecular phylogenies.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/14574403},
volume = {425},
