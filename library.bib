@article{Cuezva2002,
abstract = {Mitochondrial H+-ATP synthase is required for cellular energy provision and for efficient execution of apoptosis. Almost one century ago, Otto Warburg proposed the hypothesis that mitochondrial function might be impaired in cancer cells. However, his hypothesis was never demonstrated in human carcinomas. In this study, we have analyzed the expression of the beta-catalytic subunit of the H+-ATP synthase (beta-F1-ATPase) of mitochondria in carcinomas of the human liver, kidney, and colon. We show that carcinogenesis in the liver involves a depletion of the cellular mitochondrial content, as revealed by reduced content of mitochondrial markers, whereas in kidney and colon carcinomas, it involves a selective repression of the expression of the beta-F1-ATPase concurrent with an increase in the expression of the glycolytic glyceraldehyde-3-phosphate dehydrogenase. Both mechanisms limit mitochondrial cellular activity in cancer, strongly supporting Warburg's hypothesis, and suggest a mechanism for the resistance and compromised apoptotic potential of tumor cells. Furthermore, we show that the metabolic state of the cell, as defined by a bioenergetic mitochondrial index relative to the cellular glycolytic potential, provides a signature of carcinogenesis of prognostic value in assessing the progression of colorectal carcinomas.},
author = {Cuezva, J.M. and Krajewska, Maryla and de Heredia, M.L. and Krajewski, Stanislaw and Santamaria, G. and Kim, Hoguen and Zapata, J.M. and Marusawa, Hiroyuki and Chamorro, Margarita and Reed, J.C.},
issn = {0008-5472},
journal = {Cancer Res.},
keywords = {Animals,Carcinoma,Catalysis,Colonic Neoplasms,Colonic Neoplasms: enzymology,Hepatocellular,Hepatocellular: enzymology,Hepatocellular: pathology,Warburg,bioenergetic signature of cancer},
mendeley-tags = {Warburg,bioenergetic signature of cancer},
number = {22},
pages = {6674},
publisher = {AACR},
title = {{The bioenergetic signature of cancer: a marker of tumor progression}},
url = {http://cancerres.aacrjournals.org/cgi/content/abstract/62/22/6674},
volume = {62},
year = {2002}
}
