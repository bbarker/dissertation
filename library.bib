issn = {0006-3592},
journal = {Biotechnol. Bioeng.},
keywords = {electrospray mass,metabolome,yeast metabolism},
month = mar,
number = {7},
pages = {839--47},
shorttitle = {Biotechnol Bioeng},
title = {{Dynamics of intracellular metabolites of glycolysis and TCA cycle during cell-cycle-related oscillation in Saccharomyces cerevisiae.}},
volume = {89},
