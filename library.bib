file = {:C$\backslash$:/Users/brand\_000/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Wittmann et al. - 2005 - Dynamics of intracellular metabolites of glycolysis and TCA cycle during cell-cycle-related oscillation in Saccharomyces cerevisiae.pdf:pdf},
issn = {0006-3592},
