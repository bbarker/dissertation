file = {:C$\backslash$:/Users/brand\_000/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Heyland, Fu, Blank - 2009 - Correlation between TCA cycle flux and glucose uptake rate during respiro-fermentative growth of Saccharomyces cerevisiae.pdf:pdf},
issn = {1465-2080},
journal = {Microbiology},
keywords = {Carbon Dioxide,Carbon Dioxide: metabolism,Citric Acid Cycle,Ethanol,Ethanol: metabolism,Fermentation,Glucose,Glucose: metabolism,Glycolysis,Hydrogen-Ion Concentration,Kinetics,Models, Biological,Saccharomyces cerevisiae,Saccharomyces cerevisiae: growth \& development,Saccharomyces cerevisiae: metabolism},
month = dec,
number = {Pt 12},
pages = {3827--37},
pmid = {19684065},
title = {{Correlation between TCA cycle flux and glucose uptake rate during respiro-fermentative growth of Saccharomyces cerevisiae.}},
