keywords = {Disease,Disease: genetics,Enzymes,Enzymes: genetics,Enzymes: metabolism,Epistasis, Genetic,Humans,Metabolic Networks and Pathways,Metabolic Networks and Pathways: genetics,Models, Biological,Mutation,Phenotype,Saccharomyces cerevisiae,Saccharomyces cerevisiae: genetics,Saccharomyces cerevisiae: metabolism},
