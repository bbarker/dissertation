file = {:C$\backslash$:/Users/brand\_000/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Blank, Kuepfer, Sauer - 2005 - Large-scale 13C-flux analysis reveals mechanistic principles of metabolic network robustness to null mutations in yeast.pdf:pdf},
issn = {1465-6914},
journal = {Genome Biol.},
keywords = {Carbon,Carbon Isotopes,Carbon: metabolism,Gene Deletion,Genes, Duplicate,Genes, Duplicate: genetics,Genome, Fungal,Genome, Fungal: genetics,Glucose,Glucose: metabolism,Saccharomyces cerevisiae,Saccharomyces cerevisiae: genetics,Saccharomyces cerevisiae: metabolism},
month = jan,
