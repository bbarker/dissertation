file = {:C$\backslash$:/Users/brand\_000/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Kelley, Ideker - 2005 - Systematic interpretation of genetic interactions using protein networks.pdf:pdf},
