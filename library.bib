keywords = {CLOCK CDC epigenetic cancer},
mendeley-tags = {CLOCK CDC epigenetic cancer},
month = feb,
number = {4},
pmid = {20124474},
title = {{CLOCK in Breast Tumorigenesis: Genetic, Epigenetic, and Transcriptional Profiling Analyses.}},
url = {http://cancerres.aacrjournals.org/cgi/content/abstract/70/4/1459},
volume = {70},
year = {2010}
}
