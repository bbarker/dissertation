journal = {PLoS Genet.},
keywords = {Acidosis,Acidosis: genetics,Acidosis: metabolism,Anoxia,Breast Neoplasms,Breast Neoplasms: genetics,Breast Neoplasms: metabolism,Cells, Cultured,Energy Metabolism,Female,Gene Expression Profiling,Gene Expression Regulation, Neoplastic,Genomics,Humans,Lactic Acid,Lactic Acid: metabolism,Oligonucleotide Array Sequence Analysis},
