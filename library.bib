doi = {10.1371/journal.pcbi.1002018},
file = {:C$\backslash$:/Users/brand\_000/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Shlomi et al. - 2011 - Genome-scale metabolic modeling elucidates the role of proliferative adaptation in causing the Warburg effect.pdf:pdf},
issn = {1553-7358},
journal = {PLoS Comput. Biol.},
keywords = {Biomass,Cell Proliferation,Energy Metabolism,Gene Expression,Genome,Glutamine,Glutamine: metabolism,Glycolysis,Glycolysis: genetics,Glycolysis: physiology,Human,Humans,Metabolic Networks and Pathways,Neoplasms,Neoplasms: genetics,Neoplasms: metabolism},
month = mar,
number = {3},
pages = {e1002018},
