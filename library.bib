}
@article{Marjanska2010,
abstract = {The low sensitivity of 13C spectroscopy can be enhanced using dynamic nuclear polarization. Detection of hyperpolarized 1-(13)Cpyruvate and its metabolic products has been reported in kidney, liver, and muscle. In this work, the feasibility of measuring 13C signals of hyperpolarized 13C metabolic products in the rat brain in vivo following the injection of hyperpolarized 1-(13)Cpyruvate and 2-(13)Cpyruvate is investigated. Injection of 2-(13)Cpyruvate led to the detection of 2-(13)Clactate, but no other downstream metabolites such as TCA cycle intermediates were detected. Injection of 1-(13)Cpyruvate enabled the detection of both 1-(13)Clactate and 13Cbicarbonate. A metabolic model was used to fit the hyperpolarized 13C time courses obtained during infusion of 1-(13)Cpyruvate and to determine the values of VPDH and VLDH.},
author = {Marjańska, Małgorzata and Iltis, Isabelle and Shestov, Alexander A and Deelchand, Dinesh K and Nelson, Christopher and Uğurbil, K\^{a}mil and Henry, Pierre-Gilles},
