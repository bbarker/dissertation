journal = {Curr. Genet.},
keywords = {DNA, Intergenic,Dinucleotide Repeats,Evolution, Molecular,Genome, Fungal,Regulatory Sequences, Nucleic Acid,Repetitive Sequences, Nucleic Acid,Saccharomyces cerevisiae,Saccharomyces cerevisiae: genetics,Software},
number = {5},
