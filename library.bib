journal = {J. Cereb. Blood Flow Metab.},
keywords = {Animals,Brain,Brain: metabolism,Carbon Isotopes,Chemical,Citric Acid Cycle,Glucose,Glucose: metabolism,Glutamates,Glutamates: metabolism,Glutamic Acid,Hydrogen,Inbred Strains,Magnetic Resonance Spectroscopy,Magnetic Resonance Spectroscopy: methods,Male,Models,Phosphorus,Rats},
month = mar,
number = {2},
pages = {170--9},
pmid = {1968068},
title = {{The flux from glucose to glutamate in the rat brain in vivo as determined by 1H-observed, 13C-edited NMR spectroscopy.}},
