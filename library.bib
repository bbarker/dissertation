journal = {PLoS One},
keywords = {Adenosine Triphosphate,Adenosine Triphosphate: chemistry,Computational Biology,Computational Biology: methods,Computer Simulation,Energy Metabolism,Energy Metabolism: physiology,Humans,Ischemia,Ischemia: pathology,Metabolic Networks and Pathways,Mitochondria,Mitochondria: metabolism,Models, Biological,Models, Statistical,Muscle, Skeletal,Muscle, Skeletal: metabolism,NAD,NAD: chemistry,Oxygen,Oxygen Consumption,Oxygen Consumption: physiology,Oxygen: metabolism,Thermodynamics},
month = jan,
