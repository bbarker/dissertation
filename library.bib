keywords = {Adaptation, Biological,Adaptation, Biological: genetics,Ergosterol,Ergosterol: biosynthesis,Ergosterol: genetics,Evolution, Molecular,Gene Expression Regulation, Fungal,Gene Expression Regulation, Fungal: genetics,Gene Expression Regulation, Fungal: physiology,Genetics, Population,Models, Genetic,Quantitative Trait Loci,Saccharomyces cerevisiae,Saccharomyces cerevisiae: genetics,Selection, Genetic},
