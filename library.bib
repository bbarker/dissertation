keywords = {Astrocytes,Astrocytes: metabolism,Brain Chemistry,Brain Chemistry: physiology,Energy Metabolism,Energy Metabolism: physiology,Glucose,Glucose: metabolism,Glycolysis,Humans,Kinetics,Lactic Acid,Lactic Acid: metabolism,Models, Statistical,Neurons,Neurons: metabolism,Oxidation-Reduction,Sodium,Sodium: metabolism},
month = mar,
number = {3},
pages = {586--602},
pmid = {19888285},
publisher = {Nature Publishing Group},
title = {{Changes in glucose uptake rather than lactate shuttle take center stage in subserving neuroenergetics: evidence from mathematical modeling.}},
