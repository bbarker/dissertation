journal = {BMC Microbiol.},
keywords = {Amino Acid Motifs,Gene Expression Regulation, Bacterial,Iron,Iron: metabolism,Kinetics,Multigene Family,Oligonucleotide Array Sequence Analysis,Pseudomonas syringae,Pseudomonas syringae: growth \& development,Pseudomonas syringae: metabolism},
month = jan,
