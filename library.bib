journal = {J. Biol. Chem.},
keywords = {Amino Acid Sequence,Escherichia coli,Fungal Proteins,Fungal Proteins: chemistry,Models,Molecular,Molecular Sequence Data,Protein Structure,Quaternary,Saccharomyces cerevisiae,Saccharomyces cerevisiae: chemistry,Saccharomyces cerevisiae: enzymology,Succinate Dehydrogenase,Succinate Dehydrogenase: chemistry,Succinate Dehydrogenase: genetics},
