publisher = {Elsevier},
title = {{Ensemble modeling of metabolic networks.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2599852\&tool=pmcentrez\&rendertype=abstract},
volume = {95},
year = {2008}
