keywords = {cambrian explosion \'{a} cooperation,energetic metabolism \'{a} glycolysis,\'{a},\'{a} metazoan evolution},
month = aug,
number = {2},
pages = {145--53},
shorttitle = {J Mol Evol},
title = {{An early and anaerobic scenario for the transition to undifferentiated multicellularity.}},
volume = {67},
