issn = {1061-4036},
journal = {Nat. Genet.},
keywords = {Adaptation,Biological,Biological: genetics,Epistasis,Escherichia coli,Escherichia coli: genetics,Escherichia coli: growth \& development,Evolution,Genetic,Genetics,Likelihood Functions,Microbial,Models,Selection,Statistical Distributions,Vesicular stomatitis Indiana virus,Vesicular stomatitis Indiana virus: genetics,Vesicular stomatitis Indiana virus: growth \& devel},
month = apr,
number = {4},
pages = {555--560},
pmid = {17369829},
title = {{Distributions of epistasis in microbes fit predictions from a fitness landscape model}},
