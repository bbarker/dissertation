journal = {J. Theor. Biol.},
keywords = {Algorithms,Animals,Biological,Metabolic Networks and Pathways,Metabolome,Models,Neural Networks (Computer),Saccharomyces cerevisiae,Saccharomyces cerevisiae: metabolism},
number = {2},
pages = {311--5},
pmid = {19490860},
title = {{Flux balance analysis: a geometric perspective.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19490860},
volume = {258},
year = {2009}
}
